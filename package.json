{
        "name": "csu-mcp",
        "description": "CSU 校园 API + MCP 代理。提供教务、图书馆、校车等工具，既可作为本地 API 也可直接通过 MCP 使用",
        "private": false,
        "version": "3.1.2",
        "type": "module",
        "bin": {
                "csu-mcp": "./dist-node/bin/csu-mcp.js"
        },
        "scripts": {
                "build": "tsc -p tsconfig.json",
                "start:mcp:sse": "node --import tsx/esm mcp/sse.ts",
                "start:mcp:stdio": "node --import tsx/esm mcp/stdio.ts",
                "start:api": "node --import tsx/esm server/index.ts"
        },
        "files": [
                "package.json",
                "README.md",
                "dist-node"
        ],
        "dependencies": {
                "@modelcontextprotocol/sdk": "^1.24.0",
                "cheerio": "^1.1.2",
                "express": "^5.1.0",
                "fetch-cookie": "^3.1.0",
                "node-fetch": "^3.3.2",
                "react": "^19.2.0",
                "react-dom": "^19.2.0",
                "tough-cookie": "^6.0.0",
                "zod": "^3.24.1",
                "tsx": "^4.20.6"
        },
        "devDependencies": {
                "@eslint/js": "^9.39.1",
                "@types/express": "^5.0.5",
                "@types/node": "^24.10.1",
                "@types/react": "^19.2.5",
                "@types/react-dom": "^19.2.3",
                "@vitejs/plugin-react": "^5.1.1",
                "eslint": "^9.39.1",
                "eslint-plugin-react-hooks": "^7.0.1",
                "eslint-plugin-react-refresh": "^0.4.24",
                "globals": "^16.5.0",
                "typescript": "~5.9.3",
                "typescript-eslint": "^8.46.4",
                "vite": "^7.2.4"
        }
}
